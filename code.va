// code.va

module code;

def puts(s: string) -> bool;

def func(s: string) -> string;

def main() {
    var str = "test";
    let str2 = func(str);
    let str3 = func("test2");
    let str4 = func("åäö");

    puts(str);
    puts(str2);
    puts(str3);
    puts(str4);

    str = str2;
}

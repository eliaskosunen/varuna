// code.va

module code;

import cstd;

def main() -> i32 {
    putstr("Hello world!");
    let buf = stralloc(64i64);
    putstr("Please give some input: ");
    getstr(buf, 63);
    putstr("You gave: ");
    if putstr(buf) == eof() {
        perrorstr("");
        strfree(buf);
        return 1;
    }
    strfree(buf);
    return 0;
}

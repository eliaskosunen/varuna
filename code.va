// code.va

module code;

import cstd;

def main() -> i32 {
    puts("Hello world!\n");
    let buf = stralloc(64i64);
    puts("Please give some input: ");
    gets(buf, 63);
    puts("You gave: ");
    if puts(buf) == eof() {
        perror("");
        strfree(buf);
        return 1;
    }
    strfree(buf);
}
